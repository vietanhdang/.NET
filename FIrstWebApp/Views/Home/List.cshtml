@{
}
@using FIrstWebApp.Models
@model List<Student>
<!DOCTYPE html>
<html>
<head>
    <title>List</title>
</head>
<body>
    <h2>List Of Student @Model.Count</h2>
    @{
        foreach (Student student in Model)
        {
            <div>@student.Id - @student.Name</div>
        }
    }
</body>
</html>